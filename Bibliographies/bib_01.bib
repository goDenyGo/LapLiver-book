   issue = {4},
   journal = {Journal of hepato-biliary-pancreatic sciences},
   keywords = {laparoscopic hepatectomy á anatomical},
   pages = {435-440},
   pmid = {23269462},
   title = {Totally laparoscopic hepatectomy exposing the major vessels.},
   volume = {20},
