issn = {0003-4932},
journal = {Annals of Surgery},
keywords = {205,213,257,ann surg 2013,definitions and surgical techniques,laparoscopy,liver resection,major hepatectomy,materials and methods},
mendeley-groups = {LLR/Authors/Wakabayashi G,LapLiverBook/Chapter 1},
