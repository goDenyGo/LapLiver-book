   author = {Kuo-Feng Hsu and Tsang-Pai Liu and Jyh-Cherng Yu and Teng-Wei Chen and Ming-Lang Shih and Kuang-Ling Ou and Cheng-Jueng Chen and De-Chuan Chan and Chung-Bao Hsieh},
