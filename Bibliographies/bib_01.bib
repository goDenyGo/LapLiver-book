@article{Koffron2004,
abstract = {{\textless}h3{\textgreater}Background{\textless}/h3{\textgreater}{\textless}p{\textgreater}Recent interest in cyst fluid analysis (CFA) for carbohydrate antigen 19-9 (CA 19-9) and carcinoembryonic antigen (CEA), and the introduction of laparoscopic surgery (LS) in the management of hepatic cysts have resulted in sporadic reports of elevated CA 19-9 and CEA levels in intrahepatic biliary cystadenoma (IBC) CFA, and the application of LS in the management of simple cysts. However, the role of CA 19-9 and CEA in the diagnosis of IBC and the role of LS in the management of IBC have not been previously defined.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Methods{\textless}/h3{\textgreater}{\textless}p{\textgreater}We studied 34 patients with IBC at a single institution (8 years). The first 12 patients with IBC were managed with open surgical intervention. We subsequently developed a diagnostic and treatment algorithm for IBC that included, in the last 22 patients, prospective analysis of CFA for CA 19-9 and CEA, and LS for management{\textless}/p{\textgreater}{\textless}h3{\textgreater}Results{\textless}/h3{\textgreater}{\textless}p{\textgreater}All patients with IBC who underwent CFA had elevated CA 19-9 (range, 2247-1,757,510; N {\textless} 33 U/mL) and mildly elevated CEA (range, 3.3-212,; N {\textless} 3 ng/ml). In all 22 patients the cyst lining consisted of biliary epithelium ± mesenchymal stroma (MS). In 1/22 patients (highest CA 19-9 level), the cyst epithelium did not contain either MS or intestinal metaplasia. In contrast, control patients (simple cysts, n=8) had normal CFA. In the previous 12 patients managed with laparotomy, 6/12 patients had biliary epithelium alone; the other 6 also contained MS with 1 patient exhibiting intestinal metaplasia and cystadenocarcinoma.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Conclusions{\textless}/h3{\textgreater}{\textless}p{\textgreater}We have proposed a management algorithm for IBC that incorporates CFA and laproscopic surgical management that allows for selective minimally invasive cyst wall sampling for patients with IBC.{\textless}/p{\textgreater}},
author = {Koffron, Alan and Rao, Sambasiva and Ferrario, Mario and Abecassis, Michael},
doi = {10.1016/J.SURG.2004.06.031},
issn = {0039-6060},
journal = {Surgery},
mendeley-groups = {LapLiverBook/Chapter 2},
month = {oct},
number = {4},
pages = {926--936},
pmid = {15467680},
publisher = {Elsevier},
title = {{Intrahepatic biliary cystadenoma: Role of cyst fluid analysis and surgical management in the laparoscopic era}},
url = {https://www.surgjournal.com/article/S0039-6060(04)00427-1/fulltext},
volume = {136},
