journal = {Liver cancer},
keywords = {Anatomy of the liver,Biliary surgery,Corrosion liver casts,Liver segments,Liver surgery},
mendeley-groups = {LapLiverBook/Chapter 2,LapLiverBook/Chapter 1},
month = {feb},
