@article{Berardi2017,
abstract = {{\textless}h3{\textgreater}Background{\textless}/h3{\textgreater}{\textless}p{\textgreater}First seen as an innovation for select patients, laparoscopic liver resection (LLR) has evolved since its introduction, resulting in worldwide use. Despite this, it is still limited mainly to referral centers. The aim of this study was to evaluate a large cohort undergoing LLR from 2000 to 2015, focusing on the technical approaches, perioperative and oncologic outcomes, and evolution of practice over time.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Study Design{\textless}/h3{\textgreater}{\textless}p{\textgreater}The demographics and indications, intraoperative, perioperative, and oncologic outcomes of 2,238 patients were evaluated. Trends in practice and outcomes over time were assessed.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Results{\textless}/h3{\textgreater}{\textless}p{\textgreater}The percentage of LLR performed yearly has increased from 5{\%} in 2000 to 43{\%} in 2015. Pure laparoscopy was used in 98.3{\%} of cases. Wedge resections were the most common operation; they were predominant at the beginning of LLR and then decreased and remained steady at approximately 53{\%}. Major hepatectomies were initially uncommon, then increased and reached a stable level at approximately 16{\%}. Overall, 410 patients underwent resection in the posterosuperior segments; these were more frequent with time, and the highest percentage was in 2015 (26{\%}). Blood loss, operative time, and conversion rate improved significantly with time. The 5-year overall survival rates were 73{\%} and 54{\%} for hepatocellular carcinoma (HCC) and colorectal liver metastases (CRLM), respectively. The 5-year, recurrence-free survival rates were 50{\%} and 37{\%} for HCC and CRLM, respectively.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Conclusions{\textless}/h3{\textgreater}{\textless}p{\textgreater}Since laparoscopy was introduced, a long implementation process has been necessary to allow for standardization and improvement in surgical care, mastery of the technique, and the ability to obtain good perioperative results with safe oncologic outcomes.{\textless}/p{\textgreater}},
author = {Berardi, Giammauro and Cleven, Stijn Van and Fretland, {\AA}smund Avdem and Barkhatov, Leonid and Halls, Mark and Cipriani, Federica and Aldrighetti, Luca and Hilal, Mohammed Abu and Edwin, Bj{\o}rn and Troisi, Roberto I.},
doi = {10.1016/J.JAMCOLLSURG.2017.08.006},
file = {:Users/denys/Library/Application Support/Mendeley Desktop/Downloaded/Berardi et al. - 2017 - Evolution of Laparoscopic Liver Surgery from Innovation to Implementation to Mastery Perioperative and Oncologic.pdf:pdf},
issn = {1072-7515},
journal = {Journal of the American College of Surgeons},
keywords = {AAPC,CRLM,HCC,LLR,PS,average annual percentage change,colorectal liver metastases,hepatocellular carcinoma,laparoscopic liver resection,posterosuperior segments},
