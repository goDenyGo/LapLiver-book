issn = {1948-5182},
journal = {World Journal of Hepatology},
keywords = {Ascites,Bile leakage,Coagulation disorder,Complication,Liver failure,Renal failure,Surgical site infection},
mendeley-groups = {LapLiverBook/Chapter 2},
