file = {:Users/denys/Documents/Work/Медицинская литература/Articles/Laparoscopy/Lap Liver Resection/LapLTx-donor/2018 - Laparoscopic Donor Hepatectomy for Adult Living Donor Liver Transplantation Recipients.pdf:pdf},
issn = {15276473},
journal = {Liver Transplantation},
mendeley-groups = {LapLiverBook/Chapter 1},
number = {11},
pages = {1545--1553},
title = {{Laparoscopic Donor Hepatectomy for Adult Living Donor Liver Transplantation Recipients}},
volume = {24},
year = {2018}
}

@article{Au2018,
abstract = {Minimally invasive surgery potentially reduces operative morbidities. However, pure laparoscopic approaches to donor hepatectomy have been limited by technical complexity and concerns over donor safety. Reducedwound donor hepatectomy, either in the form of a laparoscopic-assisted technique or by utilizing a minilaparotomy wound, i.e. , hybrid approach, has been developed to bridge the transition to pure laparoscopic donor hepatectomy, offering some advantages of minimally invasive surgery. To date, pure laparoscopic donor left lateral sectionectomy has been validated for its safety and advantages and has become the standard in experienced centres. Pure laparoscopic approaches to major left and right liver donation have been reported for their technical feasibility in expert hands. Robotic-assisted donor hepatectomy also appears to be a valuable alternative to pure laparoscopic donor hepatectomy, providing additional ergonomic advantages to the surgeon. Existing reports derive from centres with tremendous experience in both laparoscopic hepatectomy and donor hepatectomy. The complexity of these procedures means an arduous transition from technical feasibility to reproducibility. Donor safety is paramount in living donor liver transplantation. Careful donor selection and adopting standardized techniques allow experienced transplant surgeons to safely accumulate experience and acquire proficiency. An international prospective registry will advance the understanding for the role and safety of pure laparoscopic donor hepatectomy.},
author = {Au, Kin Pan and Chok, Kenneth Siu Ho},
doi = {10.3748/wjg.v24.i25.2698},
file = {:Users/denys/Documents/Work/Медицинская литература/Articles/Laparoscopy/Lap Liver Resection/LapLTx-donor/2018 - Minimally invasive donor hepatectomy, are we ready for prime time.pdf:pdf},
issn = {22192840},
journal = {World Journal of Gastroenterology},
keywords = {Laparoscopic donor hepatectomy,Living donor liver transplantation,Minimally invasive surgery},
mendeley-groups = {LapLiverBook/Chapter 1},
number = {25},
pages = {2698--2709},
title = {{Minimally invasive donor hepatectomy, are we ready for prime time?}},
volume = {24},
year = {2018}
}

